{
  "name": "plugin-round-integer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "webpack && npm run deploy",
    "watch": "webpack --watch & xdpm watch ./dist -o",
    "deploy": "xdpm install ./dist -o",
    "format": "prettier --write 'src/**/*.{js,ts,vue}'",
    "validate": "xdpm validate ./dist",
    "package": "xdpm package ./dist && mv dist.xdx my-plugin.xdx"
  },
  "devDependencies": {
    "@adobe/xdpm": "^3.1.0",
    "css-loader": "^3.2.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@adobexd/typings": "https://github.com/AdobeXD/typings",
    "@types/node": "^12.12.14",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
